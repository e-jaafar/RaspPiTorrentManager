{
  "apps": [
    {
      "name": "DiscordBot",
      "script": "bot.js",
      "log_date_format": "YYYY-MM-DD HH:mm Z",
      "out_file": "/dev/null",
      "error_file": "/dev/null",
      "merge_logs": true,
      "log_type": "json",
      "autorestart": true,
      "watch": false,
      "max_memory_restart": "1G",
      "env": {
        "NODE_ENV": "production"
      }
    }
  ]
}